{
  "name": "cypress-msteams-mocha-reporter",
  "version": "1.0.0",
  "description": "A microsoft teams reporter for mocha reports generated by cypress",
  "main": "src/index.js",
  "bin": {
    "cypress-msteams-reporter": "src/index.js"
  },
  "scripts": {
    "generate:allure-report": "allure generate --clean",
    "ms-teams:report": "npx node src/index.js ",
    "ms-teams:mocha-report": "npx node src/index.js --report-type mocha --report-path ./cypress/reports/mocha/mochawesome.json",
    "cypress:tests": "cypress run --env allure=true --spec cypress/integration/msTeamsReporterTests/*.spec.js",
    "cypress:mocha-tests": "cypress run cypress/integration/msTeamsReporterTests/*.spec.js --reporter cypress-multi-reporters --reporter-options configFile=cypress-reporters/cypress-reporter.json --headless --config experimentalInteractiveRunEvents=true,screenshotsFolder=mochareports/assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gondera/cypress-msteams-mocha-reporter/.git"
  },
  "author": "Paulo Fagundes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gondera/cypress-msteams-mocha-reporter//issues"
  },
  "homepage": "https://github.com/gondera/cypress-msteams-mocha-reporter/#readme",
  "dependencies": {
    "commander": "^6.2.1",
    "dotenv": "^8.2.0",
    "ms-teams-webhook": "^1.0.4"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.0.0",
    "cypress": "^9.6.0",
    "cypress-multi-reporters": "^1.2.4",
    "cypress-mochawesome-reporter": "^3.0.1"
  }
}